{
  "name": "@borertm/missing-locales-core",
  "version": "0.0.1",
  "author": "Maxx Borer <maxxborer@gmail.com>",
  "description": "A Vite plugin that helps you find missed keys in your localization files.",
  "keywords": [
    "vite",
    "cli",
    "i18n",
    "localization",
    "locale",
    "missing",
    "keys"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "get-all": "node ./utils/get-all.js",
    "test": "jest",
    "dev": "tsup --watch src",
    "build": "tsup --dts",
    "publish": "pnpm publish",
    "release": "pnpm i && pnpm run build && pnpm run publish",
    "clear:lock": "rm -rf pnpm-lock.yaml && rm -rf yarn.lock && rm -rf package-lock.json",
    "clear:node_modules": "rm -rf node_modules",
    "clear:dependencies": "pnpm run clear:lock && pnpm run clear:node_modules"
  },
  "engines": {
    "node": "^16.0.0 || >=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/borertm/missing-locales.git",
    "directory": "packages/missing-locales"
  },
  "bugs": {
    "url": "https://github.com/borertm/missing-locales/issues"
  },
  "homepage": "https://github.com/borertm/missing-locales/packages/missing-locales-core#readme",
  "devDependencies": {
    "@types/jest": "29.2.4",
    "@types/node": "18.11.17",
    "jest": "29.3.1",
    "ts-jest": "29.0.3",
    "tsup": "6.5.0",
    "typescript": "4.9.4"
  }
}

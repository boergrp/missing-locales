{
  "name": "@borerteam/vite-plugin-missing-locales",
  "version": "1.1.0",
  "author": "Maxx Borer <maxxborer@gmail.com>",
  "description": "A Vite plugin that helps you find missed keys in your localization files.",
  "keywords": [
    "vite",
    "cli",
    "i18n",
    "localization",
    "locale",
    "missing",
    "keys"
  ],
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "tsup --watch src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsup --dts",
    "publish": "pnpm publish",
    "release": "pnpm i && pnpm run build && pnpm run publish",
    "clear:lock": "rm -rf pnpm-lock.yaml && rm -rf yarn.lock && rm -rf package-lock.json",
    "clear:node_modules": "rm -rf node_modules",
    "clear:dependencies": "pnpm run clear:lock && pnpm run clear:node_modules"
  },
  "engines": {
    "node": "^16.0.0 || >=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/borerteam/missing-locales.git",
    "directory": "packages/vite-plugin-missing-locales"
  },
  "bugs": {
    "url": "https://github.com/borerteam/missing-locales/issues"
  },
  "homepage": "https://github.com/borerteam/missing-locales/packages/vite-plugin-config-env#readme",
  "dependencies": {
    "@borerteam/cli-missing-locales": "workspace:../cli-missing-locales"
  },
  "devDependencies": {
    "@types/jest": "29.2.4",
    "@types/node": "18.11.17",
    "jest": "29.3.1",
    "ts-jest": "29.0.3",
    "tsup": "6.5.0",
    "typescript": "4.9.4"
  },
  "peerDependencies": {
    "vite": "^4.0.0"
  }
}

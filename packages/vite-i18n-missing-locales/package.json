    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "tsup-node --watch src",
    "build": "tsup-node --dts"
  },
  "engines": {
    "node": "^16.0.0 || >=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/borerteam/vite-i18n-missing-locales.git",
    "directory": "packages/vite-i18n-missing-locales"
  },
  "bugs": {
    "url": "https://github.com/borerteam/vite-i18n-missing-locales/issues"
  },
  "homepage": "https://github.com/borerteam/vite-i18n-missing-locales/packages/vite-plugin-config-env#readme",
  "devDependencies": {
    "vite": "^3.2.4"
  }
}

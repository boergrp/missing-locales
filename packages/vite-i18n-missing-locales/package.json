{
  "name": "vite-i18n-missing-locales",
  "version": "1.0.0",
  "private": true,
  "author": "Maxx Borer <maxxborer@gmail.com>",
  "description": "A Vite plugin that helps you find missed keys in your localization files.",
  "keywords": [
    "vite",
    "i18n",
    "localization",
    "missing",
    "keys"
  ],
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
